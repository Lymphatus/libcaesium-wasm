{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*",
    "target/wasm32-unknown-emscripten/release/libcaesium-wasm.js",
    "target/wasm32-unknown-emscripten/release/libcaesium_wasm.wasm"
  ]
}